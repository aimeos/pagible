{
    "name": "aimeos/laravel-nestedset",
    "description": "Nested Set Model for Laravel",
    "keywords": [
        "laravel",
        "nested sets",
        "nsm",
        "database",
        "hierarchy"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Kalnoy",
            "email": "lazychaser@gmail.com"
        },
        {
            "name": "Aimeos",
            "email": "aimeos@aimeos.org"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/events": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Aimeos\\Nestedset\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "ramsey/uuid": "^4.7"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Aimeos\\Nestedset\\NestedSetServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "@php ./vendor/bin/phpunit"
        ]
    }
}
